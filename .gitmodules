[submodule "vendor/cJSON"]
	path = vendor/cJSON
	url = https://github.com/DaveGamble/cJSON.git
[submodule "vendor/SDL_image"]
	path = vendor/SDL_image
	url = https://github.com/libsdl-org/SDL_image.git
	branch = SDL2
	ignore = dirty
[submodule "vendor/SDL"]
	path = vendor/SDL
	url = https://github.com/libsdl-org/SDL.git
	branch = SDL2
	ignore = dirty
[submodule "vendor/SDL_ttf"]
	path = vendor/SDL_ttf
	url = https://github.com/libsdl-org/SDL_ttf.git
	branch = SDL2
    ignore = dirty
[submodule "vendor/SDL2_gfx"]
	path = vendor/SDL2_gfx
	url = https://github.com/mupfdev/SDL2_gfx
	ignore = dirty
[submodule "vendor/freetype"]
	path = vendor/freetype
	url = https://github.com/freetype/freetype.git
    ignore=dirty
[submodule "rocks"]
	path = rocks
	url = git@github.com:naoxio/rocks.git
